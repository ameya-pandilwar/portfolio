<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle" >
            <corners android:radius="5dip"/>
            <stroke android:width="2dip" android:color="#36797f"/>
            <gradient android:angle="270" android:startColor="#669900" android:endColor="#99CC00" ></gradient>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="5dip"/>
            <stroke android:width="2dip" android:color="#36797f"/>
            <gradient android:angle="270" android:startColor="#99CC00" android:endColor="#669900" ></gradient>
        </shape>
    </item>
</selector>